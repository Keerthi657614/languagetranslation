    <style>
        body {
            font-family: 'Roboto', sans-serif;
            margin: 0;
            padding: 0;
            background-color: #f9f9f9;
        }
        .header {
            background-color: #10b3ad;
            color: white;
            padding: 20px;
            text-align: center;
            position: relative; /* Add relative positioning for absolute child */
        }
        .header a {
            position: absolute; /* Position link absolutely within the header */
            right: 20px; /* Space from the right edge */
            top: 50%; /* Center vertically */
            transform: translateY(-50%); /* Adjust for exact vertical center */
            color:black; /* Link color */
            text-decoration: none; /* Remove underline */
            font-size: 1.5rem; /* Font size for the link */
            padding: 10px; /* Padding for clickable area */
            border-radius: 5px; /* Rounded corners */
            transition: background-color 0.3s, color 0.3s; /* Smooth transition */
        }
        .header a:hover {
            background-color:palevioletred; /* Light background on hover */
            color: black; /* Change text color on hover */
        }
        .header h1 {
            margin: 0;
            font-size: 4rem;
        }
        .header p {
            font-size: 2rem;
            margin-top: 10px;
        }
        .navbar {
            background-color: #0073e6;
            color: white;
            padding: 10px 0;
            text-align: center;
            position: sticky;
            top: 0;
            z-index: 1000;
        }
        .navbar a {
            color: white;
            text-decoration: none;
            padding: 15px 20px;
            font-size: 1.2rem;
        }
        .navbar a:hover {
            background-color: #005bb5;
        }
        .categories {
            display: flex;
            justify-content: space-around;
            flex-wrap: wrap;
            margin: 20px;
            gap: 10px;
        }
        .category {
            background-color: #8ffaf4;
            width: calc(30% - 20px);
            text-align: center;
            border-radius: 50px;
            overflow: hidden;
            box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s, background-color 0.3s, color 0.3s;
        }
        .category:hover {
            background-color: #74afad;
            transform: scale(1.05);
            box-shadow: 0px 8px 16px rgba(0, 0, 0, 0.2);
            color: black;
        }
        .category p {
            font-weight: bold;
            padding: 10px;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        .category a {
            display: block;
            padding: 10px;
            background-color: #0073e6;
            color: white;
            text-decoration: none;
            border-radius: 0 0 50px 50px;
            transition: background-color 0.3s;
        }
        .category a:hover {
            background-color: #005bb5;
        }

        /* Ranking Color Styles */
        .ranking {
            padding: 5px;
            border-radius: 5px;
            margin: 5px 0;
            color: white;
        }
        /* Ranking Color Classes (Add more as needed) */
        
        /* Footer Styles */
        .footer {
            text-align: center;
            padding: 20px;
            background-color: #74afad;
            color: white;
            position: relative;
            bottom: 0;
            width: 100%;
        }

        @media (max-width: 768px) {
            .category {
                width: calc(45% - 20px);
            }
        }
        @media (max-width: 480px) {
            .category {
                width: calc(100% - 20px);
            }
        }
    </style>
</head>
